[alias]
  b = branch
  ci = commit
  co = checkout
  df = diff
  l = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)%an%Creset' --abbrev-commit
  st = status
[include]
  path = ~/.gitconfig.local
[core]
  editor = vim
  excludesfile = ~/.gitignore_global
